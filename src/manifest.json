{
  "package": "fun.okschoollife.miband9.beta",
  "name": "OK School Life",
  "versionName": "0.3.1",
  "versionCode": 0.1,
  "minPlatformVersion": 1000,
  "icon": "/common/assets/icon/icon-v4-final.png",
  "deviceTypeList": [
    "watch"
  ],
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.prompt"
    }
  ],
  "config": {
    "logLevel": "log",
    "designedWidth": "192",
    "designedHeight": "490"
  },
  "permissions": [
    {
      "name": "system.storage"
    },
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    }
  ],
  "resources": {
    "files": [
      "$src/common/data/events.json"
    ]
  },
  "router": {
    "entry": "pages/load",
    "pages": {
      "pages/load": {
        "component": "load"
      },
      "pages/index": {
        "component": "index"
      },
      "pages/main": {
        "component": "main"
      },
      "pages/achievement": {
        "component": "achievement"
      },
      "pages/about": {
        "component": "about"
      },
      "pages/donation": {
        "component": "donation"
      }
    }
  }
}